<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <EditText
        android:id="@+id/pref_datastore_edit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:hint="preference"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/get_pref_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get_pref"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/pref_datastore_edit" />

    <Button
        android:id="@+id/set_pref_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="set_pref"
        app:layout_constraintStart_toEndOf="@id/get_pref_btn"
        app:layout_constraintTop_toTopOf="@id/get_pref_btn" />

    <EditText
        android:id="@+id/proto_datastore_edit"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:hint="protobuf"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/get_pref_btn" />

    <Button
        android:id="@+id/get_proto_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="get_proto"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/proto_datastore_edit" />

    <Button
        android:id="@+id/set_proto_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:text="set_proto"
        app:layout_constraintStart_toEndOf="@id/get_proto_btn"
        app:layout_constraintTop_toTopOf="@id/get_proto_btn" />

    <Button
        android:id="@+id/test_sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/get_proto_btn"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        android:text="test_sharedpref"/>

    <TextView
        android:id="@+id/test_sp_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/test_sp"
        app:layout_constraintStart_toEndOf="@id/test_sp"
        app:layout_constraintBottom_toBottomOf="@id/test_sp"/>

    <Button
        android:id="@+id/test_data_store"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/test_sp"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        android:text="test_data"/>

    <TextView
        android:id="@+id/test_ds_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/test_data_store"
        app:layout_constraintStart_toEndOf="@id/test_data_store"
        app:layout_constraintBottom_toBottomOf="@id/test_data_store"/>

    <Button
        android:id="@+id/test_mmkv"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/test_data_store"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="10dp"
        android:text="test_mmkv"/>

    <TextView
        android:id="@+id/test_mmkv_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        app:layout_constraintTop_toTopOf="@id/test_mmkv"
        app:layout_constraintStart_toEndOf="@id/test_mmkv"
        app:layout_constraintBottom_toBottomOf="@id/test_mmkv"/>

</androidx.constraintlayout.widget.ConstraintLayout>